{
  "name": "fivemdiscordbot",
  "version": "1.0.0",
  "description": "A discord bot to help with your FiveM server.",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "pushcmds": "node src/regester-commands.js",
    "express": "node Web-UI/server.js",
    "dev": "nodemon src/index.js",
    "express-dev": "nodemon Web-UI/server.js",
    "petro-start": "node src/index.js --petro",
    "petro-express": "node Web-UI/server.js --petro",
    "copy-files": "copyfiles -u 1 Web-UI/**/*.html Web-UI/**/*.css Web-UI/public/**/*.* Web-UI/**/*.json dist/Web-UI/",
    "copy-config": "copyfiles -u 1 config/**/*.json dist/config/ && copyfiles -u 1 src/files/*.json dist/src/ && copyfiles .env .env.example CONTRIBUTORS.md LICENSE package-lock.json dist/",
    "build": "rimraf dist/ && tsc --build && npm run copy-files && npm run copy-config && npm run minify && node scripts/removesctrict.js",
    "watch": "tsc -w --preserveWatchOutput --skipLibCheck && nodemon Web-UI/server.js --watch dist/",
    "minify": "esbuild dist/**/*.js --minify --allow-overwrite --packages=external --outdir=dist/"
  },
  "author": "TheAFKGamer10",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheAFKGamer10/SASLEDiscordBot.git"
  },
  "funding": {
    "url": "https://afkhosting.win/"
  },
  "nodemonConfig": {
    "ignore": [
      "WEB-UI/auth/data/users.json",
      "src/files/next-rp.json"
    ]
  },
  "type": "commonjs",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "discord.js": "^14.12.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.3",
    "express-rate-limit": "^7.2.0",
    "express-session": "^1.18.0",
    "mysql2": "^3.9.4",
    "node-machine-id": "^1.1.12",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "copyfiles": "^2.4.1",
    "esbuild": "0.21.2",
    "nodemon": "^3.1.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.5"
  }
}
